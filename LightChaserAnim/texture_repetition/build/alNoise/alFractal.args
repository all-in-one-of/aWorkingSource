<param name="lacunarity" label="Lacunarity"/>
<param name="gain" label="Gain"/>
<param name="turbulent" label="Turbulent" widget="checkBox"/>
<page name="Remap">
	<param name="RMPinputMin" label="Input min"/>
	<param name="RMPinputMax" label="Input max"/>
