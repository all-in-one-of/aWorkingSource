		<param name="RMPcontrast" label="Contrast"/>
		<param name="RMPcontrastPivot" label="Pivot"/>
	</page>
	<page name="Bias and gain">
		<param name="RMPbias" label="Bias"/>
		<param name="RMPgain" label="Gain"/>
	</page>
	<param name="RMPoutputMin" label="Output min"/>
