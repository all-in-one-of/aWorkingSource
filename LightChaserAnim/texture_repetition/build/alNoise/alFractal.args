<args format="1.0">
<param name="mode" label="Mode" widget="popup">
	<hintlist name="options">
		<string value="scalar"/>
		<string value="vector"/>
	</hintlist>
</param>
<param name="space" label="Space" widget="popup">
	<hintlist name="options">
		<string value="world"/>
		<string value="object"/>
		<string value="Pref"/>
		<string value="UV"/>
	</hintlist>
</param>
<param name="scale" label="Vector"/>
<param name="frequency" label="Frequency"/>
<param name="time" label="Time"/>
